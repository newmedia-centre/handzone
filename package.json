    "tsx": "^4.3.0",
    "typed-emitter": "^2.1.0",
    "typescript": "^5.3.2",
    "vite-express": "*",
    "znv": "^0.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
