{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"definitions": {
		"RealtimeData": {
			"type": "object",
			"properties": {
				"message_size": {
					"type": "number",
					"description": "Total message length in bytes"
				},
				"time": {
					"type": "number",
					"description": "Time elapsed since the controller was started"
				},
				"q_target": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Target joint positions"
				},
				"qd_target": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Target joint velocities"
				},
				"qdd_target": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Target joint accelerations"
				},
				"i_target": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Target joint currents"
				},
				"m_target": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Target joint moments (torques)"
				},
				"q_actual": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Actual joint positions"
				},
				"qd_actual": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Actual joint velocities"
				},
				"i_actual": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Actual joint currents"
				},
				"i_control": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Joint control currents"
				},
				"tool_vector_actual": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Actual Cartesian coordinates of the tool: (x,y,z,rx,ry,rz)"
				},
				"tcp_speed_actual": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Actual speed of the tool given in Cartesian coordinates"
				},
				"tcp_force": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Generalised forces in the TCP"
				},
				"tool_vector_target": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Target Cartesian coordinates of the tool: (x,y,z,rx,ry,rz)"
				},
				"tcp_speed_target": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Target speed of the tool given in Cartesian coordinates"
				},
				"digital_input_bits": {
					"type": "number",
					"description": "Current state of the digital inputs. NOTE: these are bits encoded as int64_t, e.g. a value of 5 corresponds to bit 0 and bit 2 set high"
				},
				"motor_temperatures": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Temperature of each joint in degrees celsius"
				},
				"controller_timer": {
					"type": "number",
					"description": "Controller realtime thread execution time"
				},
				"robot_mode": {
					"type": "number",
					"description": "Robot mode"
				},
				"joint_modes": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Joint control modes"
				},
				"safety_mode": {
					"type": "number",
					"description": "Safety mode"
				},
				"tool_accelerometer_values": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Tool x,y and z accelerometer values (software version 1.7)"
				},
				"speed_scaling": {
					"type": "number",
					"description": "Speed scaling of the trajectory limiter"
				},
				"linear_momentum_norm": {
					"type": "number",
					"description": "Norm of Cartesian linear momentum"
				},
				"v_main": {
					"type": "number",
					"description": "Masterboard: Main voltage"
				},
				"v_robot": {
					"type": "number",
					"description": "Masterboard: Robot voltage (48V)"
				},
				"i_robot": {
					"type": "number",
					"description": "Masterboard: Robot current"
				},
				"v_actual": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Actual joint voltages"
				},
				"digital_outputs": {
					"type": "number",
					"description": "Digital outputs"
				},
				"program_state": {
					"type": "number",
					"description": "Program state"
				},
				"elbow_position": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Elbow position"
				},
				"elbow_velocity": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Elbow velocity"
				},
				"safety_status": {
					"type": "number",
					"description": "Safety status"
				},
				"payload_mass": {
					"type": "number",
					"description": "Payload Mass [kg]"
				},
				"payload_cog": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Payload Center of Gravity (x, y, z) [m]"
				},
				"payload_inertia": {
					"type": "array",
					"items": {
						"type": "number"
					},
					"description": "Payload Inertia (Ixx, Iyy, Izz, Ixy, Ixz, Iyz) [kg*m^2]"
				}
			},
			"required": [
				"message_size",
				"time",
				"q_target",
				"qd_target",
				"qdd_target",
				"i_target",
				"m_target",
				"q_actual",
				"qd_actual",
				"i_actual",
				"i_control",
				"tool_vector_actual",
				"tcp_speed_actual",
				"tcp_force",
				"tool_vector_target",
				"tcp_speed_target",
				"digital_input_bits",
				"motor_temperatures",
				"controller_timer",
				"robot_mode",
				"joint_modes",
				"safety_mode",
				"tool_accelerometer_values",
				"speed_scaling",
				"linear_momentum_norm",
				"v_main",
				"v_robot",
				"i_robot",
				"v_actual",
				"digital_outputs",
				"program_state",
				"elbow_position",
				"elbow_velocity",
				"safety_status",
				"payload_mass",
				"payload_cog",
				"payload_inertia"
			],
			"additionalProperties": false,
			"description": "Struct containing all of the data send using the realtime 5.10 protocol https://s3-eu-west-1.amazonaws.com/ur-support-site/16496/ClientInterfaces_Realtime.pdf"
		}
	}
}